#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
    int t;
    
    cin>>t;
    
    for(int i =0;i<t;i++){
        int n;
        
        cin>>n;
        
        int arr[n],sum=0;
        
        for(int j = 0;j<n;j++){
            cin>>arr[j];
            sum = sum + arr[j];
        }
        
        if(sum%2==0){
            cout<<"Yes";
        }
        else{
            cout<<"No";
        }
        cout<<endl;
        
    }
}

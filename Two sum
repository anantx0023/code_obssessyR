class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int t) {
        vector<int>ans;
        unordered_map<int,int>map;

        for(int i=0;i<nums.size();i++){
            if(map.count(t-nums[i])!=0){
                ans.push_back(i);
                ans.push_back(map[t-nums[i]]);

                return ans;
            }
            else{
                map[nums[i]]=i;
            }
        }
        return ans;
    }
    
};
